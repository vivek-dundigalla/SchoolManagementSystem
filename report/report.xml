<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="report_student_invoice_pdf" model="ir.actions.report">
        <field name="name">student invoice pdf report</field>
        <field name="model">accounting.feemanager</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">SchoolManagementSystem.report_fee_invoice_report_document</field>
        <field name="report_file">SchoolManagementSystem.report_fee_invoice_report_document</field>
