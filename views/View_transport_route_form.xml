<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_route_form" model="ir.ui.view">
        <field name="name">transport.route.form</field>
        <field name="model">transport.route</field>
        <field name="arch" type="xml">
            <form string="Route">
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                    </group>
                    <group>
                        <field name="start_point"/>
                        <field name="end_point"/>
                        <field name="stop_names"/>
                        <field name="stops_list" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
