<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_expense_manager_form" model="ir.ui.view">
        <field name="name">Expenses</field>
        <field name="model">accounting.manager</field>
        <field name="arch" type="xml">
            <form string="Expense">
                <sheet>
                    <group>
                        <field name="date"/>

                        <field name="names"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_expense_manager_list" model="ir.ui.view">
        <field name="name">Expenses</field>
        <field name="model">accounting.manager</field>
        <field name="arch" type="xml">
            <list string="Expenses">

                        <field name="date"/>

                        <field name="names"/>

            </list>
        </field>
    </record>



    <record id="action_accounting_expense_manager" model="ir.actions.act_window">
        <field name="name">Expenses</field>
        <field name="res_model">accounting.manager</field>
        <field name="view_mode">list,form</field>
<!--        <field name="view_id" ref="view_teacher_list"/>-->
    </record>

</odoo>